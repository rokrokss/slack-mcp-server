    "@rokrokss/slack-mcp-server-linux-arm64": "2.0.0",
    "@rokrokss/slack-mcp-server-windows-amd64": "2.0.0",
    "@rokrokss/slack-mcp-server-windows-arm64": "2.0.0"
  },
  "repository": {
